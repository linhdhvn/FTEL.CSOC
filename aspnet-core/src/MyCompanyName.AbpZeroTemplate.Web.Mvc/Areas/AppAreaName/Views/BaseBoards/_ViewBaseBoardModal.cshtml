
@using MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Models.Common.Modals
@using MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Models.BaseBoards
@model BaseBoardViewModel

@await Html.PartialAsync("~/Areas/AppAreaName/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="BaseBoardsInformationsTab">

        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("Manufacturer") :</h6>
                @Model.BaseBoard.Manufacturer
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Model") :</h6>
                @Model.BaseBoard.Model
            </div>
            <div class="col-6 mb-3">
                <h6>@L("SerialNumber") :</h6>
                @Model.BaseBoard.SerialNumber
            </div>
            <div class="col-6 mb-3">
                <h6>@L("UUID") :</h6>
                @Model.BaseBoard.UUID
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Description") :</h6>
                @Model.BaseBoard.Description
            </div>
            <div class="col-6 mb-3">
                <h6>@L("CreateTime") :</h6>
                @Model.BaseBoard.CreateTime
            </div>
            <div class="col-6 mb-3">
                <h6>@L("UpdateTime") :</h6>
                @Model.BaseBoard.UpdateTime
            </div>

        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/AppAreaName/Views/Common/Modals/_ModalFooterWithClose.cshtml")